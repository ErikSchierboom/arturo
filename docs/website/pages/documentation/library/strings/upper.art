name: "upper"
type: :function
description: "convert given string to uppercase"
module: "Strings"
line: 1058
source: "https://github.com/arturo-lang/arturo/blob/v0.9.83/src/library/Strings.nim#L1058"
returns: [
                :char
                 :string
                 :nothing
        ]
examples: [
                {
                        print upper "hello World, 你好!"       ; "HELLO WORLD, 你好!"
                }
                 {
                        str: "hello World, 你好!"
                        upper 'str                           ; str: "HELLO WORLD, 你好!"
                }
                 {
                        ch: `a`
                        upper ch    
                        ; => `A`
                }
        ]
attributes: #[
        ]
arguments: #[
        string: [
                        :char
                         :string
                         :literal
                ]
        ]
related: [
