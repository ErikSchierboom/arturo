name: "chop"
type: :function
description: "remove last item from given collection"
module: "Collections"
line: 125
source: "https://github.com/arturo-lang/arturo/blob/v0.9.83/src/library/Collections.nim#L125"
returns: [
                :string
                 :block
                 :nothing
        ]
examples: [
                {
                        print chop "books"          ; book
                        print chop chop "books"     ; boo
                }
                 {
                        str: "books"
                        chop 'str                   ; str: "book"
                }
                 "chop [1 2 3 4]              ; => [1 2 3]"
                 {
                        chop.times: 3 "Arturo"      ; Art
                }
        ]
attributes: #[
        times: #[
                types: [
                                :integer
                        ]
                description: "remove multiple items"
                ]
        ]
arguments: #[
        collection: [
                        :string
                         :literal
                         :block
                ]
        ]
